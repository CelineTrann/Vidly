@model IEnumerable<Vidly.Models.Movie>
@{
    ViewBag.Title = "Inex";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-bordered table-hover">
    <thread>
        <tr>
            <th>Movie</th>
        </tr>
    </thread>
    <tbody>
        @foreach(var movie in Model)
        {
            <tr>
                <td>@movie.Name</td>
            </tr>
        }
    </tbody>
</table>

